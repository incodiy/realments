{
    "name": "incodiy/realments",
    "description": "Laravel and React form elements package for easy form creation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Incodiy",
            "email": "info@incodiy.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Incodiy\\Realments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Incodiy\\Realments\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Incodiy\\Realments\\Providers\\RealmentsServiceProvider"
            ],
            "aliases": {
                "Realments": "Incodiy\\Realments\\Facades\\Realments"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
